<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.central.backend.mapper.KillOddsMapper">
    <select id="findList" resultType="com.central.common.model.KillOdds">
        select * from mks_kill_odds t
        <where>
            1=1
            <if test="p.lotteryId != null and  p.lotteryId != '' and  p.lotteryId != 0">
                and t.lottery_id = #{p.lotteryId}
            </if>
        </where>
    </select>

</mapper>